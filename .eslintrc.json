{
	"env": {
		"node": true,
		"commonjs": true,
		"es2021": true,
		"jquery": true,
		"browser": true
	},
	"plugins": [
		"promise"
	],
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"windows"
		],
		"quotes": [
			"warn",
			"double"
		],
		"semi": [
			"error",
			"always"
		],
		"camelcase": "error",
		"promise/catch-or-return": "error",
		"prefer-arrow-callback": "warn",
		"eqeqeq": "error",
		"no-loop-func": "warn"
	}
}